<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuuki.crm.workbench.dao.TranHistoryDao">
    <insert id="saveTranHistory">
        insert into tbl_tran_history(
        id,
        stage,
        money,
        expectedDate,
        createTime,
        createBy,
        tranId
        )values (
        #{id},
        #{stage},
        #{money},
        #{expectedDate},
        #{createTime},
        #{createBy},
        #{tranId}
        )
    </insert>

    <delete id="deleteClueRemark">
        delete from tbl_clue_remark where clueId=#{clueID}
    </delete>
</mapper>